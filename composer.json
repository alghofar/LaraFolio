{    "name": "tyloo/larafolio",    "description": "LaraFolio is a sample Application built with the amazing PHP framework Laravel 4. The main purpose is to create a CMS system with a Blog, a Portfolio, an Admin Panel, a Contact page and dynamic Content pages.",    "keywords": ["cms", "portfolio", "blog", "laravel-based"],    "license": "MIT",    "authors": [        {            "name": "Julien Bonvarlet",            "email": "jbonva@gmail.com",            "homepage": "http://www.tyloo.fr/",            "role": "Developer"        }    ],    "require": {        "laravel/framework": "4.1.*",        "thomaswelton/laravel-gravatar": "0.0.*",        "mccool/laravel-auto-presenter": "1.*"    },    "require-dev": {        "way/generators": "dev-master",        "mockery/mockery": "dev-master",        "barryvdh/laravel-ide-helper": "1.*",        "barryvdh/laravel-debugbar": "1.*"    },    "autoload": {        "classmap": [            "app/database/migrations",            "app/database/seeds"        ],        "psr-0": {            "Tyloo": "app/"        }    },    "scripts": {        "post-install-cmd": [            "php artisan clear-compiled",            "php artisan optimize"        ],        "post-update-cmd": [            "php artisan clear-compiled",            "php artisan ide-helper:generate",            "php artisan debugbar:publish",            "php artisan optimize"        ],        "post-create-project-cmd": [            "php artisan key:generate"        ]    },    "config": {        "preferred-install": "dist"    },    "minimum-stability": "dev",    "prefer-stable": true}